{
  "name": "gitlab-claude-manager",
  "version": "1.0.0",
  "description": "GitLab repository manager with Claude Code integration for AI-powered code analysis",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "npm run build && node dist/index.js",
    "dev": "tsx src/index.ts",
    "dev:watch": "nodemon --exec \"tsx src/index.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "clean:workspaces": "ts-node scripts/cleanup-workspaces.ts",
    "type-check": "tsc --noEmit",
    "next-env": "next dev --turbopack"
  },

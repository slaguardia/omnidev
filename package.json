{
  "name": "gitlab-claude-manager",
  "version": "1.0.0",
  "description": "GitLab repository manager with Claude Code integration for AI-powered code analysis",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "npm run build && node dist/index.js",
    "dev": "tsx src/index.ts",
    "dev:watch": "nodemon --exec \"tsx src/index.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "clean:workspaces": "ts-node scripts/cleanup-workspaces.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.16.0",
    "@gitbeaker/rest": "^39.16.0",
    "axios": "^1.4.0",
    "chalk": "^5.2.0",
    "chokidar": "^3.5.3",
    "commander": "^11.0.0",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.1.0",
    "glob": "^10.2.7",
    "inquirer": "^9.2.7",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "nanoid": "^4.0.2",
    "ora": "^6.3.1",
    "simple-git": "^3.19.0",
    "tmp-promise": "^3.0.3"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "gitlab",
    "git",
    "claude",
    "ai",
    "code-analysis",
    "repository-management",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT"
}

{
  "name": "omnidev",
  "version": "1.0.0",
  "description": "Agentic workflow automation for git repositories, using Claude Code",
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --no-lint",
    "start": "next start",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "npx tsc --noEmit",
    "lint": "pnpm typecheck && eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "depcheck": "depcheck --ignores=highlight.js,@react-types/shared,@vitest/coverage-v8,autoprefixer,postcss,tsx,@assets/*",
    "lint:all": "pnpm lint && pnpm prettier && pnpm depcheck",
    "lint:all:fix": "pnpm lint:fix && pnpm prettier:fix && pnpm depcheck",
    "clean:workspaces": "ts-node scripts/cleanup-workspaces.ts",
    "debug:workspaces": "node -e \"const fs = require('fs'); const path = require('path'); const workspaceDir = path.resolve(process.cwd(), 'workspaces'); console.log('Workspace directory:', workspaceDir); try { const files = fs.readdirSync(workspaceDir, { withFileTypes: true }); console.log('Contents:', files.map(f => ({ name: f.name, isDirectory: f.isDirectory() }))); const indexPath = path.join(workspaceDir, '.workspace-index.json'); if (fs.existsSync(indexPath)) { const content = fs.readFileSync(indexPath, 'utf-8'); console.log('Index file content:', content); } else { console.log('No index file found'); } } catch (e) { console.log('Error or directory does not exist:', e.message); }\"",
    "playwright:mcp": "npx @playwright/mcp@latest --browser chromium --port 8931",
    "docker:build": "docker compose build workflow-app",
    "docker:up": "docker compose up workflow-app -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "docker:dev:build": "docker compose build workflow-dev",
    "docker:dev:up": "docker compose up workflow-dev"
  },
  "dependencies": {
    "@eslint/js": "^9.29.0",
    "@gitbeaker/rest": "^39.16.0",
    "@heroui/button": "2.2.20",
    "@heroui/card": "^2.2.20",
    "@heroui/chip": "^2.2.17",
    "@heroui/code": "2.2.15",
    "@heroui/divider": "^2.2.15",
    "@heroui/input": "^2.4.20",
    "@heroui/kbd": "2.2.16",
    "@heroui/link": "2.2.17",
    "@heroui/modal": "^2.2.19",
    "@heroui/navbar": "2.2.18",
    "@heroui/select": "^2.4.22",
    "@heroui/skeleton": "^2.2.18",
    "@heroui/snippet": "^2.2.21",
    "@heroui/switch": "2.2.18",
    "@heroui/system": "^2.4.18",
    "@heroui/theme": "2.4.16",
    "@heroui/toast": "^2.0.12",
    "@heroui/tooltip": "^2.2.19",
    "@octokit/rest": "^22.0.1",
    "@react-aria/ssr": "3.9.8",
    "@react-aria/visually-hidden": "3.8.23",
    "bcryptjs": "^3.0.2",
    "clsx": "2.1.1",
    "framer-motion": "11.13.1",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.519.0",
    "nanoid": "^4.0.2",
    "next": "^15.5.9",
    "next-auth": "^4.24.11",
    "next-themes": "0.4.6",
    "otplib": "^12.0.1",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "simple-git": "^3.19.0",
    "zod": "^3.25.75"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "15.3.1",
    "@react-types/shared": "3.29.0",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "22.15.3",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^18.3.12",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "10.4.21",
    "depcheck": "^1.4.7",
    "eslint": "9.29.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-validate-filename": "^1.0.0",
    "globals": "^16.0.0",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "tailwind-variants": "0.3.0",
    "tailwindcss": "3.4.16",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.6.3",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "gitlab",
    "git",
    "claude",
    "ai",
    "code-analysis",
    "repository-management",
    "typescript",
    "nextjs"
  ],
  "author": "Your Name",
  "license": "MIT"
}
